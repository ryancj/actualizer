<div class="closing closing-2" style="background: url(<%= get_content("#{component}.background_image") %>) no-repeat center center;
background-size: cover; position: relative; -webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover;">
  <div class="container">
    <div class="row">
      <div class="col-xl-12">

        <div class="closing-2-content">
          <div class="row">
            <div class="col-xl-9">
              <h2 id="closing-cta-two-header"><%= titleize_string(get_content("#{component}.header")) %></h2>
              <p id="closing-cta-two-sub-header">
                 <%= capitalize_string(get_content("#{component}.sub_header")) %>
              </p>
              <a href="#">
                <button id="closing-cta-two-cta" type="button" class="btn btn-primary"><%= get_content("#{component}.cta") %></button>
              </a>
            </div>
          </div>
        </div>

      </div>
    </div>
  </div>
</div>
